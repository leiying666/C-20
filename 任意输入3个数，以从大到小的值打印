#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<windows.h>
int main()
{	
	int a = 0, b = 0, c = 0;
	scanf("%d%d%d",&a,&b,&c);
	if (a < b)
	{
		int d = a;
		a = b;
		b = d;
	}
	if (a < c)
	{
		int m = a;
		a = c;
		c = m;
	}
	if (b < c)
	{
		int n = b;
		b = c;
		c = n;
	}
	printf("%d %d %d", a, b, c);
	return 0;
}
